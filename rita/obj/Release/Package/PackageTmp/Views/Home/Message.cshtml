@{
    ViewBag.Title = "Message";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section title
{
<title>Сообщение для пользователя - свадьба за границей | RitaIslands</title>

<meta name="description" content="Подберите фотографа, видеографа, визажиста и других лучших специалистов прямо у нас на сайте!" />
<meta property="og:url" content="http://ritaislands.ru/svadba-za-granicey/message" />
<meta property="og:title" content="Сообщение для пользователя - свадьба за границей | RitaIslands" />
<meta property="og:description" content="Страница сообщения для партнера" />
<meta property="og:site_name" content="RitaIslands - свадьба за границей" />
<meta property="og:image" content="~/img/direct/Santorin.jpg" />
<meta name="keywords" content="изменение пароля, свадебный видеограф, фотограф на свадьбу, визажист на свадьбу, дизайнер, свадьба за границей" />
}

<!-- Contact Section -->
<section id="contact">
    <div class="container">

        <!-- Contact Form -->
        <div class="row margin-50" style="min-height: 320px;">
            <div class="span11 no-animation-effects" style=" margin-left: 30px !important; background-color: #FFFFFF; width: 1170px; margin-top: 50px;  border-top-left-radius: 6px; border-top-right-radius: 6px; border-bottom-left-radius: 6px; border-bottom-right-radius: 6px; box-shadow: #888888 1px 1px 2px 0px;">

                <div class="span12" style="border-bottom-width: 5px; margin-bottom: 10px; margin-top: 10px;">
                    <p style="width: 1120px;">
                        <b>@ViewBag.Text</b>
                    </p>
                </div>

            </div>

        </div>
        <!-- End Contact Form -->
    </div>
</section>
<!-- End Contact Section -->
